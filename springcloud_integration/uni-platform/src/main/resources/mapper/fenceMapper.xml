<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yt.uniPlatform.mapper.FenceMapper">
	<resultMap type="com.yt.uniPlatform.dataObject.FenceDO"
		id="fenceResultMap">
		<id property="id" column="id" />
		<result property="fenceId" column="fence_id" />
		<result property="fenceName" column="fence_name" />
		<result property="radius" column="radius" />
		<result property="createAt" column="create_at" />
		<result property="thTriggerAt" column="th_trigger_at" />
		<result property="triggerAt" column="trigger_at" />
		<result property="status" column="status" />
		<result property="fkTransNumber" column="fk_trans_number" />
		<result property="fkLocationCode" column="fk_location_code" />
	</resultMap>

	<select id="queryFenceListByCondition"
		parameterType="com.yt.uniPlatform.dataObject.FenceDO"
		resultMap="fenceResultMap">
		select * from fence
		<where>
			<if
				test="fence.fkTransNumber != null and fence.fkTransNumber !='' ">
				and fk_trans_number = #{fence.fkTransNumber}
			</if>
			<if
				test="fence.fkLocationCode != null and fence.fkLocationCode !='' ">
				and fk_location_code = #{fence.fkLocationCode}
			</if>
		</where>
	</select>
</mapper>