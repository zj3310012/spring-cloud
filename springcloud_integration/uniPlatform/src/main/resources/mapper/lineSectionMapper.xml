<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yt.uniPlatform.mapper.LineSectionMapper">
	<resultMap type="com.yt.uniPlatform.dataObject.LineSectionDO"
		id="lineSectionResultMap">
		<id property="id" column="id" />
		<result property="code" column="code" />
		<result property="name" column="name" />
		<result property="alias" column="alias" />
		<result property="startPointCode" column="start_point_code" />
		<result property="startPointName" column="start_point_name" />
		<result property="endPointCode" column="end_point_code" />
		<result property="endPointName" column="end_point_name" />
		<result property="velocity" column="velocity" />
		<result property="distance" column="distance" />
		<result property="takeTime" column="take_time" />
		<result property="transType" column="trans_type" />
		<result property="status" column="status" />
		<result property="verify" column="verify" />
		<result property="dataType" column="data_type" />
		<result property="creator" column="creator" />
		<result property="updateTime" column="update_time" />
		<result property="createTime" column="create_time" />
	</resultMap>

	<select id="queryLineSectionById" resultMap="lineSectionResultMap">
		select * from line_section
		where id
		= #{id}
	</select>
</mapper>			